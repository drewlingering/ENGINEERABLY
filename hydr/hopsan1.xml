      <Parameter name="Area">0.001</Parameter>
      <Parameter name="Stroke">0.1</Parameter>
      <Parameter name="Mass">2</Parameter>
    </Component>

    <!-- Electrical domain -->
    <Component type="SignalSource" name="StartButton">
      <Parameter name="Amplitude">1</Parameter>
    </Component>
    <Component type="SignalSwitch" name="LS1" />
    <Component type="SignalSwitch" name="LS2" />
    <Component type="SignalSwitch" name="PS1" />
    <Component type="LogicAND" name="Relay1" />
    <Component type="LogicOR" name="Relay2" />

    <!-- Signal to hydraulic converters -->
    <Component type="SignalToHydraulicValve" name="SOL_A_DCV1" />
