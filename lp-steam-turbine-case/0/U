dimensions      [0 1 -1 0 0 0 0];
internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type fixedValue;
        value uniform (0 0 -50);  // Adjust for axial velocity
    }

    outletFore
    {
        type zeroGradient;
    }

    outletAft
    {
        type zeroGradient;
    }

    walls
    {
        type noSlip;
    }
}
